<?xml version="1.0" encoding="UTF-8"?>
<interface domain="@GETTEXT_PACKAGE@">
  <requires lib="gtk+" version="3.12"/>
  <template class="G3JAMinAppWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">G3JAMin</property>
    <property name="icon_name">g3jamin</property>
    <property name="default-height">680</property>
    <property name="default-width">949</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="header">
        <property name="title" translatable="yes">G3JAMin Main Window</property>
        <property name="show_close_button">True</property>
        <property name="sensitive">True</property>
        <child>
          <object class="GtkMenuButton" id="gears">
            <property name="visible">True</property>
            <style>
              <class name="image-button"/>
            </style>
            <child>
              <object class="GtkImage" id="gears-icon">
                <property name="visible">True</property>
                <property name="icon-name">emblem-system-symbolic</property>
                <property name="icon-size">1</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="pack-type">end</property>
          </packing>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="G3JAMinScenes" id="scenes">
          </object>
        </child>
        <child>
          <object class="GtkStackSwitcher" id="main_stack_switcher">
            <property name="stack">main_stack</property>
            <property name="halign">GTK_ALIGN_CENTER</property>
          </object>
        </child>
        <child>
          <object class="GtkStack" id="main_stack">
            <property name="visible">false</property>
            <child>
              <object class="GtkBox" id="geq_stack">
              </object>
              <packing>
                <property name="name">geq</property>
                <property name="title" translatable="yes">30 band EQ</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox" id="spectrum_stack">
              </object>
              <packing>
                <property name="name">spectrum</property>
                <property name="title" translatable="yes">Spectrum</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox" id="comp_curves_stack">
              </object>
              <packing>
                <property name="name">comp_curves</property>
                <property name="title" translatable="yes">Compressor curves</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
          </packing>
        </child>
      </object>
    </child>
  </template>
</interface>
